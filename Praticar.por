programa {/* Crie um programa para receber dois valores inteiros e depois motrar o resultado da multiplicação e da divisao dos valores*/
  funcao inicio() {

      inteiro valor1, valor2, mult, divi
      escreva(" Informe dois valores: ")
      leia(valor1)
      leia(valor2)
      limpa()
      mult= valor1 * valor2
      divi= valor1 / valor2

      escreva("\n valor da multiplicação: ",mult)
      escreva("\n valor da divisão: ",divi)




      
    
  }
}
